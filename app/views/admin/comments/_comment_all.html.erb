<% comments.each do |comment| %>
  <tr>
    <td><%= comment.end_user.id %></td>
    <td><%= image_tag comment.end_user.get_profile_image(100,100) %></td>
    <td><%= comment.end_user.nickname %></td>
    <td><%= comment.created_at.strftime('%Y/%m/%d') %><%= comment.comment %></td>
    <!--<td>-->
      <%#= link_to "管理者削除", admin_end_user_comment_path(comment.end_user, comment), method: :delete, remote: true,
                                                "data-confirm" => "本当に消しますか？", class: "btn btn-danger" %>
    <!--</td>-->
  </tr>
<% end %>
