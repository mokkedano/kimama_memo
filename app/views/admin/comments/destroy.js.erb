$('#comment_count_<%= @post.id %>').html("<%= j(render "public/comments/comment_count", post: @post) %>");

$('#comment_area_<%= @post.id %>').html("<%= j(render "public/comments/comment", post: @post) %>");

$('#comment_index_<%= @end_user.id %>').html("<%= j(render "comments", end_user: @end_user) %>");

$('#comment_all_index_<%= @comments.ids %>').html("<%= j(render "comment_all", comments: @comments) %>");
