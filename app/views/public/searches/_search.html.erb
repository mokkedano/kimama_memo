<% if end_user_signed_in? %>
  <div class="search_form" id="form1">
    <%= form_with url: search_path, local: true, method: :get do |f| %>
    <div>
      <%= f.text_field :word, size: "50", id: "sbox" %>
      <%= f.select :range, options_for_select([["メモ",'Post'], ["カテゴリ",'Category']]), id: "sbox2" %>
      <%= f.submit "検索", class: "btn btn-info", id: "sbtn" %>
    </div>
    <% end %>
  </div>
<% end %>
