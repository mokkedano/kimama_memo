<h4>プロフィール編集</h4>


<%= form_with model: @end_user, url: end_users_information_path, local: true, method: :patch do |f| %>

  <div class="form-group">
    <%= f.label :nickname,"ニックネーム" %>
    <%= f.text_field :nickname, class: "form-control", autofocus: true, id: "end_user_name" %>
  </div>

  <div class="form-group">
    <%= f.label :profile_image,"Image" %>
    <%= f.file_field :profile_image, class: "form-control-file" %>
  </div>

  <div class="form-group">
    <%= f.label :self_introduction,"自己紹介" %>
    <%= f.text_area :self_introduction, class: "form-control", id: "end_user_introduction" %>
  </div>

  <div class="form-group">
    <%= f.submit "保存", class: "btn btn-secondary" %>
  </div>

<% end %>
